# template_cpp CMake config file
#
# This file sets the following variables:
# template_cpp_FOUND - Always TRUE.
# template_cpp_INCLUDE_DIRS - Directories containing the template_cpp include files.
# template_cpp_IDL_DIRS - Directories containing the template_cpp IDL files.
# template_cpp_LIBRARIES - Libraries needed to use template_cpp.
# template_cpp_DEFINITIONS - Compiler flags for template_cpp.
# template_cpp_VERSION - The version of template_cpp found.
# template_cpp_VERSION_MAJOR - The major version of template_cpp found.
# template_cpp_VERSION_MINOR - The minor version of template_cpp found.
# template_cpp_VERSION_REVISION - The revision version of template_cpp found.
# template_cpp_VERSION_CANDIDATE - The candidate version of template_cpp found.

message(STATUS "Found template_cpp-@PROJECT_VERSION@")
set(template_cpp_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(template_cpp_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(template_cpp_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(template_cpp_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(template_cpp_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(template_cpp_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(template_cpp_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(template_cpp_DEFINITIONS ${<dependency>_DEFINITIONS})

set(template_cpp_VERSION @PROJECT_VERSION@)
set(template_cpp_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(template_cpp_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(template_cpp_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(template_cpp_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

